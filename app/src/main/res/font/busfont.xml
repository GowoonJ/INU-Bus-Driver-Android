<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:font="@font/notosans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/notosans_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
    />

    <font
        android:font="@font/notosans_bold"
        android:fontWeight="700"
        android:fontStyle="normal"
        app:font="@font/notosans_bold"
        app:fontWeight="700"
        app:fontStyle="normal"
    />

    <font
        android:font="@font/notosans_light"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:font="@font/notosans_light"
        app:fontStyle="normal"
        app:fontWeight="200"
    />

    <font
        android:font="@font/notosans_semibold"
        android:fontWeight="500"
        android:fontStyle="normal"
        app:font="@font/notosans_semibold"
        app:fontWeight="500"
        app:fontStyle="normal"
    />


</font-family>